<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowHostBrush" Color="#F0F3F9" />
    <SolidColorBrush x:Key="WindowCaptionBackground" Color="#F0F3F9" />
    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled" Color="#F0F3F9" />
    <SolidColorBrush
        x:Key="TranspantBackground"
        Opacity="0.001"
        Color="White" />
    <AcrylicBrush
        x:Key="AppMaskAcrylicBrush"
        FallbackColor="White"
        TintColor="White"
        TintOpacity="0.5" />
    <BitmapImage x:Key="CoverPlaceholder" UriSource="ms-appx:///Assets/coverholder-Light.png" />
    <BitmapImage x:Key="ShelfEmptyImage" UriSource="ms-appx:///Assets/shelfempty-Light.png" />
    <BitmapImage x:Key="ExploreErrorImage" UriSource="ms-appx:///Assets/exploreerror-Light.png" />
    <BitmapImage x:Key="NoteEmptyImage" UriSource="ms-appx:///Assets/noteempty-Light.png" />
    <BitmapImage x:Key="SourceEmptyImage" UriSource="ms-appx:///Assets/sourceempty-Light.png" />
    <BitmapImage x:Key="DurationEmptyImage" UriSource="ms-appx:///Assets/durationempty-Light.png" />
</ResourceDictionary>
