<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowHostBrush" Color="#202020" />
    <SolidColorBrush x:Key="WindowCaptionBackground" Color="#202020" />
    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled" Color="#202020" />
    <SolidColorBrush
        x:Key="TranspantBackground"
        Opacity="0.001"
        Color="Black" />
    <AcrylicBrush
        x:Key="AppMaskAcrylicBrush"
        FallbackColor="Black"
        TintColor="#2b2b2b"
        TintOpacity="0.5" />
    <BitmapImage x:Key="CoverPlaceholder" UriSource="ms-appx:///Assets/coverholder-Dark.png" />
    <BitmapImage x:Key="ShelfEmptyImage" UriSource="ms-appx:///Assets/shelfempty-Dark.png" />
    <BitmapImage x:Key="ExploreErrorImage" UriSource="ms-appx:///Assets/exploreerror-Dark.png" />
    <BitmapImage x:Key="NoteEmptyImage" UriSource="ms-appx:///Assets/noteempty-Dark.png" />
    <BitmapImage x:Key="SourceEmptyImage" UriSource="ms-appx:///Assets/sourceempty-Dark.png" />
    <BitmapImage x:Key="DurationEmptyImage" UriSource="ms-appx:///Assets/durationempty-Dark.png" />
</ResourceDictionary>
